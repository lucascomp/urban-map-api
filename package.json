{
  "name": "urban-map-api",
  "version": "0.0.0",
  "description": "Api do Mapa de Acessibilidade Urbana",
  "main": "bin/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development nodemon bin/server.js",
    "prestart": "yarn migration:up",
    "start": "NODE_ENV=production node bin/server.js",
    "lint": "eslint --cache 'src/**'",
    "migration": "node -r dotenv/config ./node_modules/.bin/sequelize",
    "migration:create": "yarn migration migration:generate --",
    "migration:create:model": "yarn migration model:generate --",
    "migration:create:seed": "yarn migration seed:generate --",
    "migration:up": "yarn migration db:migrate",
    "migration:down": "yarn migration db:migrate:undo",
    "migration:seed": "yarn migration db:seed:all",
    "migration:unseed": "yarn migration db:seed:undo"
  },
  "author": "Lucas Alves de Sousa <lucas_comp@outlook.com>",
  "license": "UNLICENSED",
  "homepage": "https://github.com/lucascomp/urban-map-api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucascomp/urban-map-api.git"
  },
  "bugs": {
    "url": "https://github.com/lucascomp/urban-map-api/issues"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "kcors": "^2.2.2",
    "koa": "^2.8.1",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.1",
    "koa-passport": "^4.1.3",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "nodemailer": "^6.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^5.5.1",
    "uuid": "^3.3.3",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^2.0.1"
  }
}
